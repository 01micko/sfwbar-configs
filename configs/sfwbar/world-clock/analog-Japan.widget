Set jhr = Time("%H","GMT-9")
Set jmn = Time("%M","GMT-9")
Set jsc = Time("%S","GMT-9")
Set jmeri = Time("%P","GMT-9")

Set JSec = Str(Val($jsc) * 6)
Set JMin = Str(Val($jmn) * 6)
Set JHour = Str((Val($jhr)%12)*30+Val($jmn)/2)

define svg = '<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="192" height="192" viewBox="0 0 192 192">
<defs>
<image id="source-5" x="0" y="0" width="192" height="192" xlink:href="data:image/png;base64,"/>
</defs>
<use xlink:href="#source-5"/>
  <path style="stroke:#000; stroke-width:4;" d="m 96 102 0 -70" transform="rotate('+ $JHour +' 96 96)"/>
  <path style="stroke:#000; stroke-width:4;" d="m 96 108 0 -82" transform="rotate('+ $JMin +' 96 96)"/>
  <path style="stroke: #ff0000; stroke-width:2;" d="m 96 110 0 -96" transform="rotate('+ $JSec +' 96 96)"/>
</svg>'

layout {
  image {
    style = "analog"
    interval = 1000
    value = svg
    tooltip = "Tokyo\n" + $jhr + ':' + $jmn + ' ' + $jmeri
  }
}

#CSS
image#analog {
  -GtkWidget-align: 1;
  -GtkWidget-hexpand: true;
  -GtkWidget-vexpand: true;
  min-height: 120px;
  min-width: 120px;
  padding-left: 28px;
}
